<%= form_for @home do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

<ul>
  <li><% @plants.each do |plant|%>

  <%if(@selected.include?(plant)) %>
    <%= check_box_tag "home[plant_ids][]", plant.id,true %><%= plant.name %>
  <% else %>
    <%= check_box_tag "home[plant_ids][]", plant.id%><%= plant.name %>
  <% end %>

        
    <%end%>
  </li>
</ul>

  <%= f.submit 'EDIT HOME' %>
 

 
<% end %>
